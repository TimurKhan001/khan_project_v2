{
	"plugins": [
		"postcss-import",
		"postcss-nested",
		"postcss-flexbugs-fixes",
		"postcss-custom-media",
		[
			"postcss-preset-env",
			{
				"autoprefixer": {
					"flexbox": "no-2009"
				},
				"stage": 1,
				"features": {
					"custom-properties": false,
					"nesting-rules": true
				}
			}
		]
	]
}
